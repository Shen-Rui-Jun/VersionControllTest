{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "2.1-beta",
  "assemblyVersion": {
    "version": "1.0", // optional. Use when x.y for AssemblyVersionAttribute differs from the default version property.
    "precision": "revision" // optional. Use when you want a more precise assembly version than the default major.minor.
  },
  "versionHeightOffset": "5000", // optional. Use when you need to add/subtract a fixed value from the computed version height.
  "gitCommitIdShortFixedLength": 6, // optional. Set the commit ID abbreviation length.
  "gitCommitIdShortAutoMinimum": 20 // optional. Set to use the short commit ID abbreviation provided by the git repository.
}